{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "name": "splitter-api",
  "version": 2,
  "public": true,
  "installCommand": "yarn install",
  "buildCommand": "yarn build",
  "outputDirectory": "build",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "serverless.js"
    }
  ]
}
