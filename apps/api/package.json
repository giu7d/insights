{
  "name": "splitter-api",
  "version": "1.0.0",
  "main": "build/serverless.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --inspect=0.0.0.0:9991 src/server.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir build --copy-files"
  },
  "dependencies": {
    "@trpc/server": "^10.4.2",
    "fastify": "^4.10.2",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.13",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/node": "^18.11.9",
    "@vercel/node": "^2.9.6",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.6.3"
  },
  "private": true
}
