{
  "name": "splitter-mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --dev-client",
    "dev": "expo start --lan",
    "build": "expo export --output-dir ./build --platform all",
    "run:android": "expo run:android",
    "run:ios": "expo run:ios",
    "run:web": "expo start --web",
    "lint": "eslint . --ext [.ts,.tsx]",
    "lint:fix": "eslint . --ext [.ts,.tsx] --fix",
    "test": "jest --silent --ci --coverage --testLocationInResults --json --outputFile=\"report.json\"",
    "test:dev": "jest --watch --coverage=false --changedSince=origin/main",
    "test:debug": "jest -o --watch --coverage=false",
    "test:update-snapshots": "jest -u --coverage=false",
    "e2e:ios:build": "detox build -c ios.release",
    "e2e:ios:test": "detox test -c ios.release --forceExit --cleanup --headless",
    "e2e:android:build": "detox build -c android.release",
    "e2e:android:test": "detox test -c android.release --forceExit --cleanup --headless"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@tanstack/react-query": "^4.18.0",
    "@trpc/client": "^10.4.2",
    "@trpc/react-query": "^10.4.2",
    "@trpc/server": "^10.4.2",
    "expo": "~47.0.12",
    "expo-community-flipper": "^47.0.0",
    "expo-constants": "~14.0.2",
    "expo-dev-client": "~2.0.1",
    "expo-linear-gradient": "~12.0.1",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "expo-updates": "~0.15.6",
    "nativewind": "^2.0.11",
    "react": "18.1.0",
    "react-native": "0.70.5",
    "react-native-flipper": "^0.174.0",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-reanimated": "~2.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@config-plugins/detox": "^4.0.0",
    "@testing-library/react-native": "^12.0.0-rc.0",
    "@types/detox": "^18.1.0",
    "@types/jest": "^29.4.0",
    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "detox": "^20.2.0",
    "jest": "^29.4.3",
    "jest-expo": "^47.0.1",
    "jest-fetch-mock": "^3.0.3",
    "msw": "^1.0.1",
    "msw-trpc": "^1.2.0",
    "splitter-api": "*",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.6.3"
  },
  "private": true
}
